{
    "name": "speelpenning/laravel-postcode-nl",
    "description": "A Laravel client using the Postcode.nl REST API for Dutch address verification.",
    "license": "MIT",
    "authors": [
        {
            "name": "Gerben Speelpenning",
            "email": "gerben@speelpenning.nl"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/config": "7.*",
        "illuminate/contracts": "7.*",
        "illuminate/routing": "7.*",
        "illuminate/support": "7.*",
        "illuminate/validation": "7.*"
    },
    "require-dev": {
        "laravel/laravel": "7.*",
        "phpunit/phpunit": "^8.5.8",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Speelpenning\\PostcodeNl\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
